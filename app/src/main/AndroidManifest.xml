<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rocktech.humanbodyorgan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/human_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/human_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.HumanBodyOrgan">
        <activity android:name=".MaleActivity"></activity>
        <activity android:name=".FemaleActivity" />
        <activity android:name=".SearchActivity" android:parentActivityName=".BodyActivity" />
        <activity android:name=".DetailActivity" android:parentActivityName=".BodyActivity" />
        <activity android:name=".BodyActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>